local success, errorMessage = pcall(function()
    if syn and syn.request then
        syn.request(sendRequest())
    elseif request then
        request(sendRequest())
    elseif http and http.request then
        http.request(sendRequest())
    else
        error("HTTP request method not supported.")
    end
end)

if not success then
    warn("Failed to send Webhook request: " .. tostring(errorMessage))
end

