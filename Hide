local UserInputService = game:GetService("UserInputService")
local player = game.Players.LocalPlayer
local originalSizes = {}

-- Ø¯Ø§Ù„Ø© Ù„Ø­ÙØ¸ Ø§Ù„Ø£Ø­Ø¬Ø§Ù… Ø§Ù„Ø£ØµÙ„ÙŠØ©
local function saveOriginalSizes(character)
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end

    local isR6 = humanoid.RigType == Enum.HumanoidRigType.R6
    if isR6 then
        -- Ø­ÙØ¸ Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R6
        local rightLeg = character:FindFirstChild("Right Leg")
        local leftLeg = character:FindFirstChild("Left Leg")
        if rightLeg then
            originalSizes["RightLeg"] = rightLeg.Size
        end
        if leftLeg then
            originalSizes["LeftLeg"] = leftLeg.Size
        end
    else
        -- Ø­ÙØ¸ Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R15
        local rightLowerLeg = character:FindFirstChild("RightLowerLeg")
        local leftLowerLeg = character:FindFirstChild("LeftLowerLeg")
        if rightLowerLeg then
            originalSizes["RightLowerLeg"] = rightLowerLeg.Size
        end
        if leftLowerLeg then
            originalSizes["LeftLowerLeg"] = leftLowerLeg.Size
        end
    end
end

-- Ø¯Ø§Ù„Ø© Ù„Ø¥Ø±Ø¬Ø§Ø¹ Ø§Ù„Ø£Ø·Ø±Ø§Ù Ø¥Ù„Ù‰ Ø­Ø¬Ù…Ù‡Ø§ Ø§Ù„Ø·Ø¨ÙŠØ¹ÙŠ
local function resetLegs(character)
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end

    local isR6 = humanoid.RigType == Enum.HumanoidRigType.R6
    if isR6 then
        -- Ø¥Ø¹Ø§Ø¯Ø© Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R6
        local rightLeg = character:FindFirstChild("Right Leg")
        local leftLeg = character:FindFirstChild("Left Leg")
        if rightLeg and originalSizes["RightLeg"] then
            rightLeg.Size = originalSizes["RightLeg"]
        end
        if leftLeg and originalSizes["LeftLeg"] then
            leftLeg.Size = originalSizes["LeftLeg"]
        end
    else
        -- Ø¥Ø¹Ø§Ø¯Ø© Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R15
        local rightLowerLeg = character:FindFirstChild("RightLowerLeg")
        local leftLowerLeg = character:FindFirstChild("LeftLowerLeg")
        if rightLowerLeg and originalSizes["RightLowerLeg"] then
            rightLowerLeg.Size = originalSizes["RightLowerLeg"]
        end
        if leftLowerLeg and originalSizes["LeftLowerLeg"] then
            leftLowerLeg.Size = originalSizes["LeftLowerLeg"]
        end
    end
end

-- Ø¹Ù†Ø¯ Ø¥Ø¶Ø§ÙØ© Ø§Ù„Ø´Ø®ØµÙŠØ©ØŒ Ø­ÙØ¸ Ø§Ù„Ø£Ø­Ø¬Ø§Ù… Ø§Ù„Ø£ØµÙ„ÙŠØ©
player.CharacterAdded:Connect(function(character)
    character:WaitForChild("Humanoid")
    saveOriginalSizes(character)
end)

-- Ø¥Ø°Ø§ ÙƒØ§Ù†Øª Ø§Ù„Ø´Ø®ØµÙŠØ© Ù…Ø­Ù…Ù„Ø© Ø¨Ø§Ù„ÙØ¹Ù„
if player.Character then
    saveOriginalSizes(player.Character)
end

-- Ø±Ø¨Ø· Ø§Ù„ÙˆØ¸ÙŠÙØ© Ø¨Ø²Ø± F8
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end

    -- ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„Ø²Ø± Ø§Ù„Ø°ÙŠ Ø¶ØºØ·Ù‡ Ø§Ù„Ù„Ø§Ø¹Ø¨ (Ù‡Ù†Ø§ ÙŠØªÙ… ØªØ¹ÙŠÙŠÙ† Ø§Ù„Ø²Ø± Ø¹Ù„Ù‰ "F8")
    if input.KeyCode == Enum.KeyCode.F8 then
        if player.Character then
            resetLegs(player.Character)
        end
    end
end)


