            local TPSSystem = game.Workspace:FindFirstChild("TPSSystem")
            if not TPSSystem or not TPSSystem:FindFirstChild("TPS") then
                break
            end

            local GoalPST = TPSSystem.TPS.CFrame
            player.Character.HumanoidRootPart.CFrame = HRPRotation + GoalPST.Position

            -- Ø¥Ø±Ø³Ø§Ù„ Ø§Ù„Ø­Ø¯Ø« Ø§Ù„Ø£ÙˆÙ„
            local A_1 = TPSSystem.TPS
            local A_2 = player.Character:FindFirstChild("Head")
