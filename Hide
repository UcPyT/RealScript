Tab7:AddButton({
	Name = "Auto Farm",
	Callback = function()
        _G.AUTOFARM = not _G.AUTOFARM -- ØªØ´ØºÙŠÙ„/Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù€ Auto Farm
        while _G.AUTOFARM do
            wait(0.1) -- ØªÙ‚Ù„ÙŠÙ„ Ø§Ø³ØªÙ‡Ù„Ø§Ùƒ Ø§Ù„Ù…ÙˆØ§Ø±Ø¯
            -- Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† ÙˆØ¬ÙˆØ¯ Ø§Ù„Ø¹Ù†Ø§ØµØ± Ø§Ù„Ù…Ø·Ù„ÙˆØ¨Ø©
            local player = game.Players.LocalPlayer
            if not player or not player.Character or not player.Character:FindFirstChild("HumanoidRootPart") then
                break
            end
            
            local HRPRotation = player.Character.HumanoidRootPart.CFrame.Rotation
            local TPSSystem = game.Workspace:FindFirstChild("TPSSystem")
            if not TPSSystem or not TPSSystem:FindFirstChild("TPS") then
                break
            end

            local GoalPST = TPSSystem.TPS.CFrame
            player.Character.HumanoidRootPart.CFrame = HRPRotation + GoalPST.Position

            -- Ø¥Ø±Ø³Ø§Ù„ Ø§Ù„Ø­Ø¯Ø« Ø§Ù„Ø£ÙˆÙ„
            local A_1 = TPSSystem.TPS
            local A_2 = player.Character:FindFirstChild("Head")
            local TackleEvent = game.Workspace.FE.Actions:FindFirstChild("Tackle")
            if TackleEvent then
                TackleEvent:FireServer(A_1, A_2)
            end

            -- Ø¥Ø±Ø³Ø§Ù„ Ø§Ù„Ø­Ø¯Ø« Ø§Ù„Ø«Ø§Ù†ÙŠ
            local KickEvent = game.Workspace.FE.Kick:FindFirstChild("RemoteEvent2")
            if KickEvent then
                KickEvent:FireServer()
            end

            -- Ø¥Ø¹Ø¯Ø§Ø¯ Ø§Ù„ÙƒØ§Ù…ÙŠØ±Ø§ Ø¨Ù†Ø§Ø¡Ù‹ Ø¹Ù„Ù‰ ÙØ±ÙŠÙ‚ Ø§Ù„Ù„Ø§Ø¹Ø¨
            local playerGui = player:FindFirstChild("PlayerGui")
            if playerGui and playerGui:FindFirstChild("LockScript") then
                playerGui.LockScript.SetLock.Value = true
            end

            local camera = game.Workspace.CurrentCamera
            if player.TeamColor == BrickColor.new("Bright red") then
                camera.CFrame = CFrame.lookAt(camera.CFrame.Position + Vector3.new(0, 45, 0), game.Workspace.BlueGoal.Part.Position)
            elseif player.TeamColor == BrickColor.new("Bright blue") then
                camera.CFrame = CFrame.lookAt(camera.CFrame.Position + Vector3.new(0, 45, 0), game.Workspace.RedGoal.Part.Position)
            end
        end
  	end    
})

