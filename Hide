        local function modifyLegs(character)
            local humanoid = character:FindFirstChild("Humanoid")
            if not humanoid then return end

            local isR6 = humanoid.RigType == Enum.HumanoidRigType.R6
            local preferredFoot = game.Lighting:FindFirstChild(player.Name) and game.Lighting[player.Name]:FindFirstChild("PreferredFoot")
            if not preferredFoot then
                warn("PreferredFoot not found in Lighting.")
                return
            end

            if isR6 then
                -- ØªØ¹Ø¯ÙŠÙ„ Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R6
                local legName = preferredFoot.Value == 1 and "Right Leg" or "Left Leg"
                local leg = character:FindFirstChild(legName)
                if leg then
                    leg.Size = Vector3.new(Studs, 2, Studs)
                    leg.Transparency = 0 -- Ø¹ÙˆØ¯Ø© Ø§Ù„Ø´ÙØ§ÙÙŠØ© Ø¥Ù„Ù‰ Ø§Ù„ÙˆØ¶Ø¹ Ø§Ù„Ø·Ø¨ÙŠØ¹ÙŠ
                end
            else
                -- ØªØ¹Ø¯ÙŠÙ„ Ø§Ù„Ø£Ø·Ø±Ø§Ù ÙÙŠ R15
                local legName = preferredFoot.Value == 1 and "RightLowerLeg" or "LeftLowerLeg"
                local leg = character:FindFirstChild(legName)
                if leg then
                    leg.Size = Vector3.new(Studs, 2, Studs)
                    leg.Transparency = 0 -- Ø¹ÙˆØ¯Ø© Ø§Ù„Ø´ÙØ§ÙÙŠØ© Ø¥Ù„Ù‰ Ø§Ù„ÙˆØ¶Ø¹ Ø§Ù„Ø·Ø¨ÙŠØ¹ÙŠ
                end
            end
        end

        if player.Character then
            modifyLegs(player.Character)
        end
    end
})

